  "rust-analyzer.cargo.features": "all",
  "markdownlint.config": {
    "commands-show-output": false,
    "no-duplicate-heading": false
  },
  "cSpell.enabled": true,
  "cSpell.words": [
    "alnum",
    "BINPATH",
    "canonicalize",
    "chrono",
    "Clippy",
    "datetime",
    "factorio",
    "httpmock",
    "indoc",
    "itertools",
    "Localised",
    "MSRV",
    "mymod",
    "nixpkgs",
    "numtide",
    "pastable",
    "printdoc",
    "println",
    "pycoalprocessing",
    "pyfusionenergy",
    "pyindustry",
    "reqwest",
    "rpassword",
    "rustfmt",
    "rustup",
    "serde",
    "Swatinem",
    "tempdir",
    "tempfile",
    "thiserror",
    "unversioned",
    "writedoc"
  ],
  "files.exclude": {
    ".direnv": true,
    "target": true
  }
}
